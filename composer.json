{
    "name": "elnurxf/omnipay-azericard",
    "description": "AzeriCard e-commerce gateway for Omnipay payment processing library",
    "keywords": ["omnipay", "azericard", "payment", "gateway", "azerbaijan"],
    "homepage": "https://github.com/elnurxf/omnipay-azericard",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Elnur Akhundov",
            "email": "elnurxf@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "omnipay/common": "^3.0",
        "symfony/http-client": "^7.3",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8",
        "php-http/guzzle7-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\AzeriCard\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
